script/source = "extends Spatial

signal camara_1
signal camara_2

signal allow
signal disallow

onready var child = get_node(\"Spatial\").get_node(\"running\").get_node(\"Tree\")
onready var secret = get_node(\"Spatial\").get_node(\"running\").get_node(\"Secret\")

export(NodePath) var running
onready var node_running = get_node(\"Spatial/running/Tree\")

func _ready():
	pass
	
func _process(_delta):
	get_input_keyboard()
	
func get_input_keyboard():
	if (Input.is_action_pressed(\"q\")):
		emit_signal(\"camara_1\")
	elif (Input.is_action_pressed(\"f\")):
		emit_signal(\"camara_2\")
	elif (Input.is_action_pressed(\"l\")):
		get_node(\"AnimationPlayer\").play(\"light\")
	elif (Input.is_action_pressed(\"c\")):
		get_node(\"AnimationPlayer2\").play(\"camara\")
	elif (Input.is_action_pressed(\"r\")):
		get_node(\"AnimationPlayer3\").play(\"run\")
		

func _on_Spatial_camara_1():
	$CameraGimbal/InnerGimbal/Camera.current=true


func _on_Spatial_allow():
	get_node(\"Spatial\").get_node(\"running\").add_child(child)


func _on_Spatial_disallow():
	get_node(\"Spatial\").get_node(\"running\").remove_child(child)


func _on_Area_body_exited(body):
	if (body.get_name() == \"Player\"):
		emit_signal(\"allow\")
		if Global.get_number_items() > 0:
			get_node(\"Spatial\").get_node(\"running\").remove_child(secret)
		if Global.get_number_items() < 1:
			get_node(\"Spatial\").get_node(\"running\").add_child(secret)


func _on_Area_body_entered(body):
	if (body.get_name() == \"Player\"):
		emit_signal(\"disallow\")
		if Global.get_number_items() > 0:
			get_node(\"Spatial\").get_node(\"running\").remove_child(secret)
		if Global.get_number_items() < 1:
			get_node(\"Spatial\").get_node(\"running\").add_child(secret)
"

