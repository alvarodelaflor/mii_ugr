CREATE TABLE Camisetas (ID VARCHAR(255) NOT NULL, IMAGE VARCHAR(255), MODELO VARCHAR(255), PRECIO DOUBLE, TALLA VARCHAR(255), VENDIDA TINYINT(1) default 0, PRIMARY KEY (ID))
CREATE TABLE Usuarios (DNI VARCHAR(255) NOT NULL, APELLIDOS VARCHAR(255), EMAIL VARCHAR(255), ESADMINISTRADOR TINYINT(1) default 0, NOMBRE VARCHAR(255), PRIMARY KEY (DNI))
